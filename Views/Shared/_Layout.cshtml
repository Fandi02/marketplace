										<a class="mobile-hide search-bar-icon" href="#"><i
												class="fas fa-search"></i></a>
										<ul>
											<li><a href="shop.html"><i class="fas fa-person"></i></a>
												<ul class="sub-menu">
													@if (ViewBag.IsLogged != null && ViewBag.IsLogged)
													{
														<li><a asp-area="" asp-controller="AccountCustomer"
															asp-action="Logout">Keluar</a></li>
													}
													else
													{
														<li><a asp-area="" asp-controller="AccountCustomer"
															asp-action="Login">Masuk</a></li>
														<li><a a asp-area="" asp-controller="AccountCustomer"
															asp-action="Login">Daftar</a></li>

													}
												</ul>
											</li>
										</ul>
									</div>
								</li>
								@if (ViewBag.IsLogged != null && ViewBag.IsLogged)
								{
									<div class="icon-header-item cl2 hov-cl1 trans-04 p-l-22 p-r-11 icon-header-noti js-show-cart"
									data-notify="2">
										<i class="zmdi zmdi-shopping-cart"></i>
									</div>
									<li class="nav-item">
										<a class="nav-link text-dark" asp-area="" asp-controller="Keranjang"
										asp-action="Index">Keranjang</a>
									</li>
									<li class="nav-item">
										<a class="nav-link text-dark" asp-area="" asp-controller="AccountCustomerController"
										asp-action="Logout">Log out</a>
									</li>
								}
							</ul>
						</nav>
						<a class="mobile-show search-bar-icon" href="#"><i class="fas fa-search"></i></a>
						<div class="mobile-menu"></div>
						<!-- menu end -->
					</div>
				</div>
			</div>
		</div>
	</div>
	<!-- end header -->

	<!-- search area -->
	<div class="search-area">
		<div class="container">
			<div class="row">
				<div class="col-lg-12">
					<span class="close-btn"><i class="fas fa-window-close"></i></span>
					<div class="search-bar">
						<div class="search-bar-tablecell">
							<h3>Search For:</h3>
							<input type="text" placeholder="Keywords">
							<button type="submit">Search <i class="fas fa-search"></i></button>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<!-- end search area -->

	@RenderBody()



